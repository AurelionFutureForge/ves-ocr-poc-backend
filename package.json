{
  "name": "ves-ocr-backend",
  "version": "1.0.0",
  "description": "VES OCR Backend Service",
  "main": "server.js",
  "scripts": {
    "start": "node dist/src/server.js",
    "dev": "npm run prisma:generate && nodemon -L --watch src --exec ts-node src/server.ts",
    "build": "npm run clean && npm run prisma:generate && npx tsc && tsc-alias && npm run copy-files",
    "tunnel": "lt --port 8002 --subdomain kitty-magic-chat-backend",
    "test": "jest",
    "test:verbose": "jest --verbose",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "copy-files": "copyfiles -u 1 src/config/swagger/*.yaml dist/src && copyfiles -u 1 src/api/v1/views/*.ejs dist/src",
    "clean": "rm -rf dist",
    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "db:init": "npm run prisma:generate",
    "postinstall": "prisma generate",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org edu-engineering-4f --project node-express ./dist && sentry-cli sourcemaps upload --org edu-engineering-4f --project node-express ./dist",
    "typecheck": "tsc --noEmit",
    "seed": "ts-node prisma/seed.ts"
  },
  "author": "Venkatesan",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/vision": "^5.3.4",
    "@prisma/client": "^6.16.2",
    "@prisma/extension-accelerate": "^1.2.1",
    "@prisma/extension-optimize": "^1.1.4",
    "@sentry/cli": "^2.31.2",
    "@sentry/node": "^7.118.0",
    "@sentry/profiling-node": "^10.12.0",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "amqplib": "^0.10.9",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.5",
    "path-to-regexp": "^8.2.0",
    "razorpay": "^2.9.4",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.0",
    "tesseract.js": "^6.0.1",
    "uuid": "^10.0.0",
    "winston": "^3.17.0",
    "yamljs": "^0.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.14",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/nodemailer": "^6.4.14",
    "@types/nodemon": "^1.19.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.24.2",
    "eslint": "^9.26.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^6.16.2",
    "supertest": "^7.1.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.15",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.2.2"
  }
}
